function show_hide_tabs(more_less){
	if(more_less=='more'){
		$("#more").css({display:"none"});
		$("#less").css({display:"block"});
		$(".show_hide").css({display:"block"});
	}
	else{
		$("#more").css({display:"block"});
		$("#less").css({display:"none"});
		$(".show_hide").css({display:"none"});
	}
}
	

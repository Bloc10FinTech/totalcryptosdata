						<div class="col-md-6">
							<h4>
								<a href="volume-24-hour-market/<%=product.display_name.replace('/','')%>">
								<%=product.display_name%>
								</a>
								<b>&nbsp;<% if(parseFloat(product.ticker.variation)>0){ %>
								<span style="color:green;"><%=parseFloat(product.ticker.variation).toFixed(2)%>%</span>
								<%}else if(product.ticker.variation!=undefined){ %>
								<span style="color:red;"><%=parseFloat(product.ticker.variation).toFixed(2)%>%</span>
								<%}%>
								</b>	
							</h4>
							<h4>
							<%=parseFloat(product.ticker.price).toFixed(2)%>&nbsp;<%=product.quote_currency%>
							</h4>
							<h4>Volume:&nbsp;<%=parseFloat(product.ticker.volume).toFixed(2)%> <%=product.base_currency%></h4>
						</div>
						<div class="col-md-4">
							<br/>
							<div class="chart_<%=product.id%>"><input type="hidden" class="hidden_<%=product.id%>" value="<%=product.ticker.chart%>"/></div>
						</div>
						<div class="col-md-2">
						</div>
					 </div>	

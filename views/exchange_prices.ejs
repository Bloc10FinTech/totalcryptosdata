<script>

$(document).ready(function(){
	//PROCESS TO FETCH TOTAL CRYPTO DATA
	show_tab_data('<%= tab %>');
});

function initialize_table(){
	$('.exchange').DataTable({
        "paging":   false, 
        "info":     false,
        "searching": false
    });
}

function draw_chart(){
	$("#market_summary").children('tr').each(function(){
		var val=$("#hidden_"+this.id).val();
		if(val){
			$("#chart_"+this.id).html('');
			$("#chart_"+this.id).sparkline(val.split(","), {type: 'line',width: '100px',height: '20',lineColor: '#ff6439',fillColor: "transparent"});
		}
	});
}


function show_tab_data(tab){
	if($('#market_summary').children('tr').length==0){ 
		//PROCESS TO SHOW LOADER
		var cols=$('#market_summary').parent().children('thead').children('tr').children('th').length;
		$('#market_summary').append('<tr><td colspan="'+cols+'" style="text-align:center;"><img src="/images/data-loader.gif"/></td></tr>');
		
		$.ajax({
			type:'GET',
			url:'/tabData/'+tab,
			success:function(data){ 
				var str='';
				switch(tab){
					case 'totalcryptospriceusd':
						data.forEach(function(summary,index){
						var even_odd_class='even';
						if(index%2==0){
							even_odd_class='odd';
						}
						str+='<tr role="row" class="'+even_odd_class+'" id="'+summary.product+'_totalcryptos_usd">';
						str+='<td class="sorting_1">'+(index+1)+'</td>';
						str+='<td class="text-bold">';
						str+='<img src="/images/currencies/'+summary.base_currency+'.png" width="16px" height="16px" onError="this.src=\'/images/currency.png\'" />';
						str+='<a href="/volume-24-hour-currency/'+summary.base_currency.toUpperCase()+'">';
						str+=summary.base_currency.toUpperCase();
						str+='</a>';
						str+='</td>';
						str+='<td class="text-bold" id="'+summary.product+'_price_totalcryptos_usd">';
						if(summary.change_perc_1h>0){
						str+='<span class="market-up-text">'+parseFloat(summary.price).toFixed(8)+'</span>';
						}else{
						str+='<span class="market-down-text">'+parseFloat(summary.price).toFixed(8)+'</span>';
						}
						str+='</td>';
						str+='<td id="'+summary.product+'_vol_totalcryptos_usd">'+Math.round(parseFloat(summary.volume))+'</td>';
						str+='<td id="'+summary.product+'_high_totalcryptos_usd">'+parseFloat(summary.high).toFixed(8)+'</td>';
						str+='<td id="'+summary.product+'_low_totalcryptos_usd">'+parseFloat(summary.low).toFixed(8)+'</td>';
						str+='<td id="'+summary.product+'_change_perc_1h_totalcryptos_usd">';
						if(summary.change_perc_1h>0){
						str+=parseFloat(summary.change_perc_1h).toFixed(2)+'<img src="/images/green-up-arrow.png" alt="green arrow">';
						}else{
						str+=parseFloat(summary.change_perc_1h).toFixed(2)+'<img src="/images/red-down-arrow.png" alt="red arrow">';
						}
						str+='</td>';
						str+='<td id="'+summary.product+'_change_perc_24h_totalcryptos_usd">';
						if(summary.change_perc_24h>0){
						str+='<span class="market-up-text">'+parseFloat(summary.change_perc_24h).toFixed(2)+'</span>';
						}else{
						str+='<span class="market-down-text">'+parseFloat(summary.change_perc_24h).toFixed(2)+'</span>';
						}
						str+='</td>';

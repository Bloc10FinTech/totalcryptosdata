<script>
var whether_total_crypto_table_initialized=false;
$(document).ready(function(){
	//PROCESS TO FETCH TOTAL CRYPTO DATA
	show_tab_data('<%= tab %>');
});

function initialize_table(currency=null){ 
	$('.exchange').DataTable({
        "paging":   false, 
        "info":     false,
        //"searching": false,
		"aaSorting": [],
		dom: 'Bfrtip',
        buttons: [
            'csv', 'excel', 'pdf', 'print'
        ],
		initComplete : function() { 
			if(currency){
				$('.dataTables_filter input').val(currency);
			}
			$('.dataTables_filter input').unbind(),
			self = this.api(),
			$searchButton = $('<button>')
			   .text('search')
			   .click(function() { 
				  //self.search(input.val()).draw();
				  show_tab_data('<%= tab %>',$('.dataTables_filter input').val());
			   }),
            $clearButton = $('<button>')
			   .text('clear')
			   .click(function() {
				 show_tab_data('<%= tab %>');
			   }), 
			$('.dataTables_filter').append($searchButton, $clearButton);
			
			$('.dataTables_filter input').keyup(function(){ 
				if($(this).val()==''){
					show_tab_data('<%= tab %>');
				}
			});
		}
    });
}

function format_numbers(number){
	return number.replace(/\d(?=(\d{3})+\.)/g, '$&,');
}

function draw_chart(){
	$("#market_summary").children('tr').each(function(){
		var val=$("#hidden_"+this.id).val();
		if(val){
			$("#chart_"+this.id).html('');
			$("#chart_"+this.id).sparkline(val.split(","), {type: 'line',width: '100px',height: '20',lineColor: '#ff6439',fillColor: "transparent"});
		}
	});
}

